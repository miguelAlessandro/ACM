#include <bits/stdc++.h>
using namespace std;

const int N = 100005;
int n;
int a[N], b[N];

int merge(int lo, int hi){
    int size = (hi-lo)+1;
    int mid = (hi+lo)/2;
    int inv = 0;

    int i = lo, j = mid+1, k = 0;
    while(i <= mid and j <= hi)
    	if(a[i] < a[j])
   			b[k++] = a[i++];
   		else
   			b[k++] = a[j++], inv += mid+1-i;
   	
   	while(i <= mid) b[k++] = a[i++];
   	while(j <= hi) b[k++] = a[j++];    
        
    for(int i = 0; i < size; ++i)
        a[lo+i] = b[i];
    
    return inv;
}

int merge_sort(int lo, int hi){
    int inv = 0;
    if(lo < hi){
        int mid = (lo+hi)/2;
        inv  = merge_sort(lo, mid); 
        inv += merge_sort(mid+1, hi);
        inv += merge(lo, hi);
    }
    return inv;
}

int main(){

    while(scanf("%d", &n) != EOF){ 
        for(int i = 0; i < n; ++i)
            scanf("%d%*s", a+i);

        printf("%d\n", merge_sort(0, n-1));
    }

    return 0;
}
