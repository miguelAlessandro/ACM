#include <bits/stdc++.h>

using namespace std;

int p, q, r, s, t, u;

double eps = 1e-7;

double function(const double x){
    return p*exp(-x) + q*sin(x) + r*cos(x) + s*tan(x) + t*x*x + u;
}

bool proof(const double x, const double y){
    return function(x)*function(y) > 0;   
}

double binary_search(double low, double upper){
    static double mid;
    while(low + eps < upper){
        mid = (upper + low)/2.0f;
        if(proof(low, mid)) low = mid;
        else upper = mid;
    }
    return (upper + low)/2.0f;
}

int main(){

    while(scanf("%d %d %d %d %d %d", &p, &q, &r, &s, &t, &u) != EOF)
        proof(0.0000f, 1.0000f) ?
            puts("No solution") : printf("%.4lf\n", binary_search(0.0000f, 1.0000f));

    return 0;
}
